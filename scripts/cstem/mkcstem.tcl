probe_os() {
  config_os=unknown

  uname >/dev/null 2>&1
  if test $? -eq 127; then
    return
  fi

  uname_s=`(uname -s) 2>/dev/null` || uname_s=unknown

  if test "x$uname_s" = x; then
    uname_s=unknown
  fi
  uname_s_lc=`echo "$uname_s" | tr [A-Z] [a-z]`

  case "$uname_s_lc" in
    netbsd)
      config_os=netbsd
      ;;
    bitrig)
      config_os=bitrig
      ;;
    openbsd)
      config_os=openbsd
      ;;
