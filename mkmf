#!/bin/sh
# JUMP TO MODERN SHELL IF POSSIBLE
(test_exit_code_127_sdfjsdfksjdhfksjdhf  >/dev/null) 2>/dev/null
if test $? -ne 127; then
  if test -z "$FORMAKE_BASH_REDIRECT"; then
    FORMAKE_BASH_REDIRECTED=1
    export FORMAKE_BASH_REDIRECTED
    if test -x /bin/bash; then
      exec /bin/bash "$0" ${1+"$@"}
    elif test -x /usr/bin/bash; then
      exec /usr/bin/bash "$0" ${1+"$@"}
    elif test -x /usr/local/bin/bash; then
      exec /usr/local/bin/bash "$0" ${1+"$@"}
    else
      (bash -c "pwd" >/dev/null) 2>/dev/null
      if test $? -eq 0; then
        exec bash "$0" ${1+"$@"}
      fi
    fi
  fi
fi
FORMAKE_BASH_REDIRECTED=
export FORMAKE_BASH_REDIRECTED

################################################################################
# Generated by Formake mkmkmf
# Thu Jun  1 04:01:35 EEST 2017
# mkmkmf -srcroot ./ -inst-bin scripts/abspath scripts/cstem scripts/excom scripts/ldlshell scripts/ldlwrap scripts/libro scripts/mkconfigure scripts/mkdep scripts/mkmkmf scripts/mkrule scripts/optool scripts/pofig scripts/relpath scripts/src2obj

################################################################################
# BASE CONFIGURATION
BUILDROOT=.
CONF="build.conf"
MAKEFILE="Makefile"
TOOLS="relpath mkrule"

################################################################################
# INIT
SCRIPT=`basename $0`
PATH=$PWD/$BUILDROOT/mk:$PATH
excom -f $TOOLS || exit 1
SRCDIR=`dirname $0 | xargs relpath ./`
SRCROOT=`relpath ./ $SRCDIR/$BUILDROOT`
ABS_BUILDDIR=`pwd`
SUBDIR=`relpath $BUILDROOT ./`

################################################################################
# READ CONFIG
test -f $BUILDROOT/$CONF && . $BUILDROOT/$CONF

################################################################################
# INIT MAKEFILE
mkrule init >$MAKEFILE
mkrule distclean $CONF configure.log mk/*.conf $MAKEFILE >>$MAKEFILE

################################################################################
# INSTALLATION FILES
#
# Install files by 'make install'
#
install_bin="scripts/abspath scripts/cstem scripts/excom scripts/ldlshell scripts/ldlwrap scripts/libro scripts/mkconfigure scripts/mkdep scripts/mkmkmf scripts/mkrule scripts/optool scripts/pofig scripts/relpath scripts/src2obj"
mkrule install bin $install_bin >>$MAKEFILE

################################################################################
# DIST
#
# Create distribution tarball by 'make dist'

DIST_FILES="configure mk/excom mk/relpath mk/mkrule mkmf CHANGELOG COPYING README $install_bin"
mkrule dist -n $PACKAGE_NAME -ver $PACKAGE_VERSION -buildroot $BUILDROOT -srcdir $SRCDIR -f $DIST_FILES >>$MAKEFILE || exit 1

################################################################################
# DONE
echo "$SUBDIR/$SCRIPT: DONE"

