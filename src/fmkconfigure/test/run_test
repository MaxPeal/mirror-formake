#!/bin/sh

./fmkconfigure -name my_app -ver 1.0.0 -build-h -syscap "-lX11 -lXt X11/Xlib.h X11/X.h X11/Intrinsic.h -cstr 'Widget *w = NULL;' -def __linux__ __unix__ __GNUC__=4 '#ifdef __linux__ && defined __unix__' -type Widget int32_t -func XtCreateWidget fprintf atoi -rval __GNUC__ __linux__ \$SRCROOT/test.c '#if (__GNUC__ == 4 && __GNUC_MINOR__ == 7) || __SIZEOF_LONG_LONG__ == 8'" -cc -c++ -lib -params="-modules -debug -enable-ui -ui-type -xflags -xparam -xxparam" -syscap "grep tclsh" -sh 'test -f /etc/debian_version && echo "CONFIG_DEBIAN=1" >> $CONF' -syscap "-lXm Xm/Xm.h Xm/PushB.h -func XmCreatePushButton -var xmPushButtonWidgetClass -sym xmTextFieldWidgetClass -rval XmSET -enum XmSET XmTOGGLEBUTTON" -syscap "-lgif gif_lib.h" -syscap "-with-cxx -lQtGui -inc QtGui/QtGui"

